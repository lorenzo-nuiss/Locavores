	<footer class="text-center text-lg-start">

			<section class="contenu-footer">
				<div class="container text-center text-md-start mt-5">
					<!-- Grid row -->
					<div
						class="row mt-3">
						<!-- Grid column -->
						<div class="col-md-3 mx-auto mb-4 contact">

							<h6 class="text-uppercase fw-bold mb-4">
								Contact
							</h6>

							{% if not contactInfo %}
							<p>
							Les informations de contacts ne sont pas disponibles pour le moment.
							</p>

							{% else %}
							{% if contactInfo.email %}
							<p>
								<i class="bi bi-envelope-open"></i>
                                {{ contactInfo.email}}
							</p>
							{% endif %}


								{% if contactInfo.telFixe %}
							<p>
								<i class="bi bi-telephone"></i>
								{{ contactInfo.telFixe}}
								</p>
							{% endif %}

								{% if contactInfo.telPortable %}
							<p>
								<i class="bi bi-telephone"></i>
								{{ contactInfo.telPortable}}
								</p>
							{% endif %}
							{% endif %}

						</div>

						<div class="col-md-3 ps-lg-5 mb-4 page">
							<h6 class="text-uppercase fw-bold mb-4">
								Pages
							</h6>
							<p>
								<a href="{{ path('home')}}" class="text-reset">Accueil</a>
							</p>
							<p>
								<a href="{{ path('contact')}}" class="text-reset">Contact</a>
							</p>
						</div>


						<div class="col-md-3 mx-auto mb-4 page">
							<h6 class="text-uppercase fw-bold mb-4">
								Nos catégories
							</h6>
                                    {% for categorie in categories %}
                                        <p>
                                        <a class="text-reset" href="{{ path('listAgriByCategorie', {'categorie_name': categorie.nom|replace({' ': '_'}) }) }}">
                                        {{ categorie.nom }}
                                        </a>
                                        </p>
                                    {% endfor %}
						</div>

						<div class="col-md-3 mx-auto mb-4">
							<h6 class="text-uppercase fw-bold mb-4">
								Nos reseaux
							</h6>
							<div>
								<a href="" class="me-4 text-reset">
									<i class="bi bi-facebook"></i>
								</a>
								<a href="" class="me-4 text-reset">
									<i class="bi bi-twitter"></i>
								</a>
								<a href="" class="me-4 text-reset">
									<i class="bi bi-instagram"></i>
								</a>
							</div>
						</div>
					</div>
				</div>
			</section>
			<!-- Section: Links  -->

			<!-- Copyright -->
			<div class="text-center p-4 copyright">
				
							<span class="pe-4">
								<a href="{{ path('mention')}}" class="text-reset">Mentions legales</a>
							</span>
							<span class="pe-4">
								<a href="{{ path('politiqueConfidentialite')}}" class="text-reset">Politique de confidentialité </a>
							</span>

							<span class="pe-4">
				© 2021 Copyright
							</span>
			</div>
			<!-- Copyright -->
		</footer>